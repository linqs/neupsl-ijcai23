{
  "options": {
    "runtime.db.intids": true,
    "runtime.log.level": "TRACE",
    "categoricalevaluator.categoryindexes": 1,
    "categoricalevaluator.defaultpredicate": "HasCat",
    "admmreasoner.computeperiod": 100,
    "admmreasoner.epsilonabs": 1.0e-6,
    "admmreasoner.epsilonrel": 1.0e-4,
    "admmreasoner.maxiterations": 100,
    "admmreasoner.stepsize": 1.0,
    "gradientdescent.numsteps": 1,
    "gradientdescent.runfulliterations": true,
    "gradientdescent.stepsize": 1.0e-3
  },
  "rules": [
    "1.0: Neural(Paper, Label) = HasCat(Paper, Label) ^2",
    "1.0: 0 * Link(Paper1, Paper2) + HasCat(Paper1, Label) = HasCat(Paper2, Label) ^2",
    "HasCat(Paper, +Label) = 1 ."
  ],
  "predicates": {
    "Neural/2": {
      "type": "DeepPredicate",
      "options": {
        "model-path": "../scripts/neupsl-model.py::CitationModel",
        "entity-data-map-path": "../data/citeseer/0/entity-data-map.txt",
        "entity-argument-indexes": "0",
        "class-size": 6,
        "load-path": "../data/citeseer/0/saved-networks/simple/pre-trained-tf",
        "save-path": "../data/citeseer/0/saved-networks/simple/nesy-trained-tf",
        "alpha": 0.5
      },
      "targets": {
        "learn": [
          "../data/citeseer/0/category-target-train.txt",
          "../data/citeseer/0/category-target-test.txt",
          "../data/citeseer/0/category-target-valid.txt",
          "../data/citeseer/0/category-target-latent.txt"
        ],
        "infer": [
          "../data/citeseer/0/category-target-train.txt",
          "../data/citeseer/0/category-target-test.txt",
          "../data/citeseer/0/category-target-valid.txt",
          "../data/citeseer/0/category-target-latent.txt"
        ]
      }
    },
    "Link/2": {
      "observations": {
        "learn": [
          "../data/citeseer/0/edges.txt"
        ],
        "infer": [
          "../data/citeseer/0/edges.txt"
        ]
      }
    },
    "HasCat/2": {
      "evaluations": [
        {
          "evaluator": "CategoricalEvaluator",
          "options": {
            "categoricalevaluator.categoryindexes": 1,
            "eval.includeobs": true
          }
        }
      ],
      "observations": {
        "infer": [
          "../data/citeseer/0/category-truth-train.txt",
          "../data/citeseer/0/category-truth-valid.txt"
        ]
      },
      "targets": {
        "learn": [
          "../data/citeseer/0/category-target-train.txt",
          "../data/citeseer/0/category-target-test.txt",
          "../data/citeseer/0/category-target-valid.txt",
          "../data/citeseer/0/category-target-latent.txt"
        ],
        "infer": [
          "../data/citeseer/0/category-target-test.txt",
          "../data/citeseer/0/category-target-latent.txt"
        ]
      },
      "truth": {
          "learn": [
          "../data/citeseer/0/category-truth-train.txt"
          ],
          "infer": [
          "../data/citeseer/0/category-truth-test.txt"
          ]
      }
    }
  }
}